    instructor         TEXT,
    learning_obj       TEXT,
    course_contents    TEXT,
    [teaching-methods] TEXT,
    prerequisites      TEXT,
    readings           TEXT,
    applicability      TEXT,
    workload           TEXT,
    credits            TEXT,
    evaluation         TEXT,
    time               TEXT,
    frequency          TEXT,
    duration           TEXT,
    course_type        TEXT,
    remarks            TEXT
);

